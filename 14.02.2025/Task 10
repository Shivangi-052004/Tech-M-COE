// Get the Maths score of a student. If the mark is 100, display "Phenomenal!" . 
Else if the mark is greater than 85, display "Excellent!". 
Else, if the mark is between 71 to 85, display "Good work!".
Else, if the mark is between 51 to 70, display "Practice well!". 
Else, display "Hard work is needed"


let num =prompt("Enter the maths score:");
let mathScore = Number(num);
if(mathScore == 100)
{
    console.log("Phenomenal");
}
else if(mathScore >=85 )
{
    console.log("Excellent");
}
else if(mathScore > 71 && mathScore <85)
{
    console.log("Goodwork");
}
else if(mathScore >  50 && mathScore < 70)
{
    console.log("Practise well");
}
else
{
    console.log("Hardwork is needed");
}
